@model PanelAdminaViewModel
<h1>Panel admina</h1>
<br />
<h2>Kategorie</h2>
<div class="table-wrapper-scroll-y scrollbar-secondary">
    <table class="table table-bordered table-striped table-sm ">
        <thead>
            <tr>
                <th scope="col">ID</th>
                <th scope="col">Nazwa</th>
                <th scope="col">ID właściciela</th>
                <th scope="col">Nazwa właściciela</th>
                <th scope="col">Publiczna</th>
                <th scope="col">Pytania</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var category in Model.Categories)
            {
            <tr>
                <td>@category.Id</td>
                <td>@category.Name</td>
                <td>@category.OwnerId</td>
                <td>@category.OwnerUsername</td>
                <td>@category.IsPublic</td>
                <td>@category.QuestionsCount</td>
                <td><a asp-action="DeleteCategory" asp-route-categoryId="@category.Id">Delete</a></td>
            </tr>
            }
        </tbody>
    </table>
</div>